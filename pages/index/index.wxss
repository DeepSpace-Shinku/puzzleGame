.board {
    width: 600rpx;
    justify-content: center;
    margin:-11rpx 75rpx 0% 75rpx;
    display: flex;
    flex-wrap: wrap;
}

.board.cell {
    border:1px solid black;
}

.gadgets {
    margin:0rpx 75rpx 10rpx 75rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.timer{
    min-width:140rpx;
    height:70rpx;
    color:#fff;
    line-height: 1.7em;
    border-radius: 1em;
    background:#36563C;
    display: flex;
    align-items: center;
    justify-content: center;
    border:1px solid black;
  }

.showOriginal{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 210rpx;
    height: 70rpx;
    color: white;
    background-color: #36563C;
    border:1px solid black;
  }

  .title {
      letter-spacing: 3rpx;
      font-size: 85rpx;
      font-family:-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
      color:white;
      text-shadow:5px 1px 6px black;
  }

page{
    background-color: #5DAC81;
}
